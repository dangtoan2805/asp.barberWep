<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="shortcut icon" href="~/Images/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="~/Images/apple-touch-icon.png">
    @Styles.Render("~/Content/Admin/font-css")
    @Styles.Render("~/Content/Admin/bootstrap")
    @Styles.Render("~/Content/Admin/vendor")
    @RenderSection("css", required: false)
</head>
<body class="animsition">
    <div class="page-wrapper">
        <!--SideBar-->
        @{ Html.RenderPartial("_SideBar");}
        <div class="page-container">
            <!--Header-->
            @{ Html.RenderPartial("_HeaderAdmin");}
            <!--Main-->
            @RenderBody()
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery-3")
    @Scripts.Render("~/bundles/bootstrap-4.1")
    @Scripts.Render("~/bundles/bootstrap-popper")
    @Scripts.Render("~/bundles/vendor")
    @RenderSection("scripts", required: false)
</body>
</html>
